<Window x:Class="Front_Running.Welcome_compliance"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Front_Running"
        mc:Ignorable="d"
        Title="Welcome_compliance" Height="300" Width="300">
    <Window.Background>
        <ImageBrush ImageSource="citiBG2.jpg" Opacity="1"/>

    </Window.Background>
    <Grid>

        <TextBlock x:Name="WelcomeHeading" FontSize="40" VerticalAlignment="Top" Foreground="AntiqueWhite" FontStretch="ExtraCondensed"><Run Text="Welcome"/></TextBlock>

        <TextBlock  HorizontalAlignment="Left" Foreground="AntiqueWhite" FontSize="40" x:Name="TextBlockName"  VerticalAlignment="Center" Margin="200,0"  FontStretch="ExtraCondensed" />


        <Button x:Name="button" Background="AntiqueWhite" Content="GENERATE REPORT" HorizontalAlignment="Center" Margin="103,98,0,0" VerticalAlignment="Top" Width="169" Click="button_Click" Height="43"/>
        <!--<TextBlock>

                 <Hyperlink Click="http://localhost:8080/DisplayCheck/login.jsp" FontSize="15" FontWeight="Bold" Foreground= "AntiqueWhite" FontStyle="Normal">GENERATE REPORT</Hyperlink>

        </TextBlock>-->
        

    </Grid>
</Window>
